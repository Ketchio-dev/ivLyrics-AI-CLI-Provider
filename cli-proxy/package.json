{
  "name": "ivlyrics-cli-proxy",
  "version": "2.1.0",
  "description": "Local proxy server for CLI AI tools (Claude Code, Codex CLI, Gemini CLI)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js --dev",
    "cleanup:gemini-oauth-env": "bash scripts/cleanup-gemini-oauth-env.sh",
    "cleanup:gemini-oauth-env:windows": "powershell -ExecutionPolicy Bypass -File .\\scripts\\cleanup-gemini-oauth-env.ps1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "google-auth-library": "^9.0.0"
  }
}
